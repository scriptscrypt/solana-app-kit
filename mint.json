{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Solana App Kit",
  "logo": {
    "light": "/docs/assets/light.svg",
    "dark": "/docs/assets/dark.svg"
  },
  "layout": "sidenav",
  "favicon": "/docs/assets/favicon.png",
  "sidebar": {
    "items": "border"
  },
  "colors": {
    "primary": "#4079ED",
    "light": "#64C6FF",
    "dark": "#4079ED"
  },
  "background": {
    "style": "windows"
  },
  "codeBlock": {
    "mode": "auto"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
